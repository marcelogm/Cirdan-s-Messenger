<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.CNewFriendship">
  <children>
    <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <HBox prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="NEVER">
          <children>
            <TextField fx:id="txfSearch" prefWidth="200.0" promptText="Digite o nome que deseja pesquisar" text="" HBox.hgrow="ALWAYS">
              <stylesheets>
                <URL value="@../../stylesheets/text-input.css" />
              </stylesheets>
              <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
              </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#searchAction" text="Pesquisar">
              <HBox.margin>
                <Insets right="10.0" top="10.0" />
              </HBox.margin>
            </Button>
          </children>
          <VBox.margin>
            <Insets />
          </VBox.margin>
        </HBox>
        <ListView fx:id="lvwProfiles" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      </children>
    </VBox>
  </children>
</AnchorPane>
