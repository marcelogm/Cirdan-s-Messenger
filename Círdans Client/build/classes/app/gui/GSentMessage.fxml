<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Pane prefWidth="50.0" />
            <VBox style="-fx-background-color: #EEF2ED; -fx-border-color: #A9EE93;">
               <children>
                  <HBox>
                     <children>
                        <Text fx:id="lblName" strokeType="OUTSIDE" strokeWidth="0.0" text="Nome" />
                        <Text fx:id="lblDate" fill="#a8a8a8" strokeType="OUTSIDE" strokeWidth="0.0" text="Hora">
                           <HBox.margin>
                              <Insets left="5.0" top="2.0" />
                           </HBox.margin>
                           <font>
                              <Font size="10.0" />
                           </font>
                        </Text>
                     </children>
                     <VBox.margin>
                        <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                     </VBox.margin>
                  </HBox>
                  <Text fx:id="lblMessage" strokeType="OUTSIDE" strokeWidth="0.0" text="Texto">
                     <VBox.margin>
                        <Insets bottom="1.0" left="1.0" right="1.0" top="3.0" />
                     </VBox.margin>
                  </Text>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</AnchorPane>
